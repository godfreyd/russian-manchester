{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Product",
  "description": "Description of the product",
  "type": "object",
  "properties": {
    "id": {
      "description": "Unique identity of the product",
      "type": "string",
      "maxLength": 250
    },
    "modelId": {
      "type": "string",
      "maxLength": 250
    },
    "material": {
      "description": "Material",
      "type": "string",
      "maxLength": 250
    },
    "price": {
      "description": "Price",
      "type": "integer",
      "minimum": 0
    },
    "inventory": {
      "description": "Private inventory number",
      "type": "string",
      "maxLength": 250
    },
    "brand": {
      "description": "Brand",
      "type": "string",
      "maxLength": 250
    },
    "note": {
      "type": "string"
    },
    "discount": {
      "description": "Discount",
      "type": "integer",
      "minimum": 0
    },
    "status": {
      "type": "string",
      "maxLength": 250
    }
  },
  "required": [
    "id"
  ],
  "additionalProperties": false
}
