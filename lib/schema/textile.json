{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Textile",
  "description": "Description of the textile",
  "type": "object",
  "properties": {
    "id": {
      "description": "Unique identity of the textile",
      "type": "string",
      "maxLength": 250
    },
    "goodId": {
      "type": "string",
      "maxLength": 250
    },
    "category": {
      "type": "string",
      "maxLength": 250
    },
    "inventory": {
      "description": "Private inventory number",
      "type": "string",
      "maxLength": 250
    },
    "material": {
      "description": "Material",
      "type": "string",
      "maxLength": 250
    },
    "brand": {
      "description": "Brand",
      "type": "string",
      "maxLength": 250
    },
    "note": {
      "type": "string"
    },
    "cityOfShipment": {
      "description": "The city of shipment",
      "type": "string",
      "maxLength": 250
    },
    "regionOfShipment": {
      "description": "The region of shipment",
      "type": "string",
      "maxLength": 250
    },
    "latOfShipment": {
      "type": "string",
      "maxLength": 250
    },
    "lonOfShipment": {
      "type": "string",
      "maxLength": 250
    },
    "discount": {
      "type": "string",
      "maxLength": 250
    },
    "status": {
      "type": "string",
      "maxLength": 250
    }
  },
  "required": [
    "id"
  ],
  "additionalProperties": false
}
